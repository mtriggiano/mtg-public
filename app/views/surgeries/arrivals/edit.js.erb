<% simple_nested_form_for arrival do |f| %>
	<% @builder = f %>
<% end %>
$("table.parent-table > tbody").html("<%= j render '/surgeries/shared_details_form', {f: @builder, colspan: 10} %>");
<% unless arrival.entity_id.nil? %>
	$("#surgeries_arrival_entity_id").val("<%= arrival.entity_id %>").trigger('change');
<% end %>
